<%- include('./partials/head.ejs') %>
</head>
<body>
    <%- include('./partials/navigation') %>
    <main>
        <h3>Welkom op mijn woordenboekenwebsite!</h3>
        <br>
        <h4>Hongaars - Nederlands</h4>
        <br>
        <%  wsch.sort(function(a,b) { if (a.woord < b.woord) {return -1;} if (a.woord > b.woord) { return 1;} }) %>
        <% for (let i of wsch) { %>
            <div class="d-flex flex-row">
                <div class="p-1 m-1 kaart"><%= i.woord%></div>
                <div class="p-1 m-1">-</div>
                <div class="p-1 m-1 vertaling"><%= i.vertaling %></div>
              </div>
        <% } %>
        <br>
        <h4>Nederlands - Hongaars</h4>
<%- include('./partials/end.ejs') %>
